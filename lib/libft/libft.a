#include "../inc/libft.h"

void	*ft_memchr(const void *s, int c, size_t n)
{
	size_t			i;
	unsigned char	uc;
	unsigned char	*us;

	uc = (unsigned char)c;
	us = (unsigned char *)s;
	i = 0;
	if (n == 0)
		return (NULL);
	while (us[i] != uc && i < n - 1)
		i++;
	if (us[i] == uc)
		return (us + i);
	return (NULL);
}

