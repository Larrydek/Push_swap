#include "../inc/libft.h"
#include <stdio.h>
#include <string.h>

char	*ft_memcpy(void *dst, const void *src, size_t n)
{
	size_t			i;
	unsigned char	*result;
	unsigned char	*cast_src;

	i = 0;
	result = (unsigned char *)dst;
	cast_src = (unsigned char *)src;
	if (dst == 0 && src == 0)
		return (dst);
	while (i < n)
	{
		result[i] = cast_src[i];
		i++;
	}
	return (dst);
}

