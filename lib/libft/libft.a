#include "../inc/libft.h"

char	*ft_strtrim(char const *s1, char const *set)
{
	size_t	i;
	size_t	j;
	char	*cast_s1;

	i = 0;
	j = ft_strlen(s1) - 1;
	cast_s1 = (char *)s1;
	if (!s1[0])
		return (ft_strdup("\0"));
	while (ft_strchr(set, s1[i]) != NULL && s1[i])
		i++;
	while (ft_strchr(set, s1[j]) != NULL && j > 0)
		j--;
	return (ft_substr(cast_s1, i, j - i + 1));
}

