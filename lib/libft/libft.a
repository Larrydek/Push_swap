#include "../inc/libft.h"

char	*ft_strrchr(const char *s, int c)
{
	char	*str;
	int		i;

	i = ft_strlen(s);
	str = (char *)s;
	while (str[i] != (char)c && i > 0)
		i--;
	if (str[i] == (char)c)
		return (str + i);
	else
		return (NULL);
}

