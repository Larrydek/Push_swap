#include "../inc/libft.h"

static long	ft_count_spaces(long n)
{
	long	count;

	if (!n)
		return (0);
	count = 1;
	if (n < 0)
	{
		count++;
		n = -n;
	}
	while (n)
	{
		n = n / 10;
		count++;
	}
	return (count);
}

char	*ft_itoa(int n)
{
	char	*result;
	long	i;
	long	num;

	num = n;
	if (num == 0)
		return (ft_strdup("0"));
	i = ft_count_spaces(num);
	result = malloc(sizeof(char) * i);
	if (!result)
		return (NULL);
	result[--i] = 0;
	if (num < 0)
	{
		result[0] = '-';
		num = -num;
	}
	while (num > 0)
	{
		result[--i] = (num % 10) + 48;
		num = num / 10;
	}
	return (result);
}

