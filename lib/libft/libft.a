	len_haystack = ft_strlen(haystack);
	len_needle = ft_strlen(needle);
	if (needle[i] == 0)
		return ((char *)haystack);
	if (len == 0 || haystack[0] == 0 || len_needle > len_haystack)
		return (NULL);
	while (i < len)
	{
		while (haystack[i + j] == needle[j] && i + j < len)
		{
			j++;
			if (j == len_needle)
				return ((char *)haystack + i);
		}
		i++;
		j = 0;
	}
	return (NULL);
}

