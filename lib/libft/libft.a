#include "../inc/libft.h"
#include <stdio.h>
#include <string.h>

void	*ft_memmove(void *dst, const void *src, size_t len)
{
	size_t			i;
	unsigned char	*result;
	unsigned char	*cast_src;

	i = 0;
	result = (unsigned char *)dst;
	cast_src = (unsigned char *)src;
	if (!src && !dst)
		return (dst);
	if (dst < src)
	{
		while (i < len)
		{
			result[i] = cast_src[i];
			i++;
		}
	}
	else
	{
		i = len;
		while (i-- > 0)
			result[i] = cast_src[i];
	}
	return (dst);
}
