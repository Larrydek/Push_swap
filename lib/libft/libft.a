#include <stdio.h>
#include "../inc/libft.h"

void	*ft_bzero(void *s, size_t n)
{
	size_t	i;
	char	*result;

	i = 0;
	result = (char *)s;
	if (n == 0)
		return (NULL);
	while (i < n)
	{
		result[i] = 0;
		i++;
	}
	return (NULL);
}

