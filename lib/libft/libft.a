size_t	ft_strlcat(char *dst, const char *src, size_t dstsize)
{
	size_t	len_dst;
	size_t	i;

	i = 0;
	len_dst = ft_strlen(dst);
	while ((len_dst + i + 1) < dstsize && (i < ft_strlen(src)))
	{
		dst[len_dst + i] = src[i];
