#include "../inc/libft.h"

void	*ft_calloc(size_t count, size_t size)
{
	int	*matrix;

	matrix = (int *)malloc(count * size);
	if (!matrix)
		return (NULL);
	ft_memset(matrix, 0, count * size);
	return (matrix);
}
